clock {
  timezone EST+5EDT;
  starttime '2000-01-01 0:00:00';
  stoptime '2000-01-01 0:00:00';
};
#set relax_naming_rules=1
#set profiler=1
module powerflow {
  solver_method NR;
  line_capacitance TRUE;
};
module generators;
module residential;
module climate;
module tape;

object climate {
  name "RegionalWeather";
  tmyfile "WA-Yakima.tmy2";
  interpolate QUADRATIC;
};

